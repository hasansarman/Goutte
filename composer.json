{
    "name": "fabpot/goutte",
    "type": "application",
    "description": "A simple PHP Web Scraper",
    "keywords": ["scraper"],
    "homepage": "https://github.com/FriendsOfPHP/Goutte",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/dom-crawler": "*",
        "symfony/http-client": "*",
        "symfony/mime": "*"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "*"
    },
    "autoload": {
        "psr-4": { "Goutte\\": "Goutte" },
        "exclude-from-classmap": ["Goutte/Tests"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
